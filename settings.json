{
  "vim.easymotion": true,
  "vim.sneak": false,
  "diffEditor.renderSideBySide": false,
  "editor.renderWhitespace": "all",
  "workbench.iconTheme": "vs-minimal",
  "workbench.colorTheme": "Monokai",
  "workbench.editor.showTabs": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "search.useReplacePreview": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "flowJiraCommitPrefix.pattern": "/^(feat|fix)|((?!([A-Z0-9]{1,10})-?$)[A-Z]{1}[A-Z0-9]+-\\d+)/g",
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", ";"],
      "after": ["$", "%"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Leader>", "m", "a"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "2", "s"]
    },
    {
      "before": ["<Leader>", "m", "p"],
      "commands": ["bookmarks.jumpToPrevious"]
    },
    {
      "before": ["<Leader>", "m", "l"],
      "commands": ["bookmarks.listFromAllFiles"]
    },
    {
      "before": ["T"],
      "commands": ["open-buffers.openBuffer"]
    },
    {
      "before": ["<Leader>", "m", "n"],
      "commands": ["bookmarks.jumpToNext"]
    },
    {
      "before": ["<Leader>", "e"],
      "commands": ["workbench.files.action.focusFilesExplorer"]
    },
    {
      "before": ["<Leader>", "q"],
      "commands": ["workbench.action.quit"]
    },
    {
      "before": ["<Leader>", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<Leader>", "D"],
      "commands": ["workbench.action.editorLayoutSingle"]
    },
    {
      "before": ["<Leader>", "d"],
      "commands": ["workbench.action.splitEditorRight"]
    },
    {
      "before": ["<Leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["<command-r>"],
      "commands": ["redo"]
    },
    {
      "before": ["F"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<Leader>", "y"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<Leader>", "j"],
      "commands": ["editor.action.joinLines"]
    },
    {
      "before": ["J"],
      "commands": ["workbench.action.navigateLeft"]
    },
    {
      "before": ["K"],
      "commands": ["workbench.action.navigateRight"]
    },
    {
      "before": ["<Leader>", "c", "n"],
      "commands": ["merge-conflict.next"]
    },
    {
      "before": ["<Leader>", "c", "p"],
      "commands": ["merge-conflict.previous"]
    },
    {
      "before": ["<Leader>", "b"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<Leader>", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<Leader>", "f"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["<Leader>", "k", "s"],
      "commands": ["workbench.view.scm"]
    },
    {
      "before": ["<Leader>", "k", "p"],
      "commands": ["git.push"]
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"]
    },
    {
      "before": ["g", "s"],
      "commands": ["editor.action.revealDefinitionAside"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["<Leader>", "o"],
      "commands": ["workbench.action.replaceInFiles"]
    },
    {
      "before": ["<Leader>", "i"],
      "commands": ["editor.action.startFindReplaceAction"]
    },
    {
      "before": ["<leader>", "h", "u"],
      "commands": ["git.revertSelectedRanges"]
    },
    {
      "before": ["<leader>", "h", "p"],
      "commands": ["workbench.action.editor.previousChange"]
    },
    {
      "before": ["<leader>", "h", "n"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["<leader>", "e", "n"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["<leader>", "e", "p"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["<leader>", "="],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<Leader>", "t"],
      "commands": ["workbench.action.togglePanel"]
    },
    {
      "before": ["<Leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["ctrl", "n"],
      "commands": ["workbench.action.files.newUntitledFile"]
    },
    {
      "before": ["<Leader>", "c"],
      "commands": ["workbench.action.closeAllGroups"]
    },
    {
      "before": ["<Return>"],
      "after": ["@", "q"]
    },
    {
      "before": ["="],
      "after": [],
      "commands": [
        {
          "command": "editor.action.reindentlines",
          "args": []
        }
      ]
    }
  ],
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.renderControlCharacters": true,
  "git.autofetch": true,
  "editor.suggestSelection": "first",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "files.insertFinalNewline": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "sideBarSectionHeader.foreground": "#d7ca17"
  },
  "timeline.excludeSources": ["timeline.localHistory"],
  "breadcrumbs.enabled": false,
  "window.zoomLevel": -1
}
